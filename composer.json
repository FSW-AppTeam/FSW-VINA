{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:UtrechtUniversity/HTS-AppTeam-LaravelBase.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:UtrechtUniversity/HTS-AppTeam-Livewire.git"
        }
    ],
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.8",
        "league/csv": "^9.0",
        "livewire/livewire": "^v3.2.5",
        "reliese/laravel": "^1.2",
        "spatie/laravel-ray": "^1.36",
        "utrecht_university/hts_appteam_laravelbase": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.9",
        "laravel/dusk": "^8.2",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.18",
        "laravel/ui": "^4.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan vendor:publish --force --tag=livewire:assets --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "php artisan vendor:publish --provider='UU\\HTSAppTeam\\LaravelBase\\Providers\\AppTeamServiceProvider'",
            "php artisan vendor:publish --tag=\"public\" --force --provider='UU\\HTSAppTeam\\LaravelBase\\Providers\\AppTeamServiceProvider'"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
